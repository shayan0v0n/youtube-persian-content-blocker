{
    "name": "youtube-persian-content-blocker",
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "icons": { 
      "16": "src/assets/icons/icon32.png",
      "32": "src/assets/icons/icon32.png",
      "48": "src/assets/icons/icon32.png",
      "128": "src/assets/icons/icon32.png"
    },
    "description": "this is youtube-persian-content-blocker",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "src/popup/popup.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "declarativeContent",
      "nativeMessaging"
    ]
  }
  